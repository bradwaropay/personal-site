<%- @partial('start') %>
  <% for entry in @getCollection("entries").toJSON(): %>
    <article id="<%= entry.basename %>-th" class="work-entry pure-u-1 pure-u-sm-1-2">
      <a href="<%= entry.url %>">
        <header>
          <h1 class="fit-text"><%= entry.title %></h1>
          <span class="category-tag"><%= entry.category %></span>
        </header>
      </a>
    </article>
  <% end %>
<%- @partial('end') %>
