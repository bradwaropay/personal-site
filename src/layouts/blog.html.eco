<%- @partial('start') %>
  <div class="post-list pure-u-1">
    <% for post in @getCollection("posts").toJSON(): %>
      <a href="<%= post.url %>">
        <article class="post">
          <time datetime="<%= @robotDate(post.date) %>"><%= @humanDate(post.date) %></time>
          <span class="category-tag"><%= post.category %></span>
          <h1><%= post.title %></h1>
          <h2><%= post.subtitle %></h2>
        </article>
      </a>
    <% end %>
  </div>
<%- @partial('end') %>
