---
title: "Work"
section: "work"
isPage: true
---
<% for work in @getCollection("work").toJSON(): %>
  <article class="work-item coming-soon pure-u-1 pure-u-sm-1-2">
    <a href="<%= work.url %>">
      <header>
        <h1 class="fit-text"><%= work.title %></h1>
        <p><%= work.description %></p>
      </header>
    </a>
  </article>
<% end %>
