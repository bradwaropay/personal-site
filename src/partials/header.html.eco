<header class="main-header">
  <div class="row row--full">
    <nav class="main-header__nav">
      <a href="/" class="main-header__logo"><%- @partial("svg/logo_waropay") %></a>
      <ul class="main-header__nav__list">
        <% for navItem in @getCollection("nav").toJSON(): %>
          <li class="main-header__nav__list__item"><a href="<%= navItem.url %>"><%= navItem.title %></a></li>
        <% end %>
      </ul>
    </nav>
  </div>
</header>
