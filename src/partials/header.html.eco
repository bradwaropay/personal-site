<header class="content-section main-header">
  <nav class="content-section__inner main-nav pure-menu pure-menu-horizontal">
    <ul class="list-inline pure-menu-list">
      <% for page in @getCollection("navLinks").toJSON(): %>
        <li class="pure-menu-item <%= if page.id is @document.id then 'active' else 'inactive' %>">
          <a href="<%= page.url %>">
            <%= page.title %>
          </a>
        </li>
      <% end %>
    </ul>
  </nav>
</header>
