<header class="content-section main-header">
  <nav class="content-section__inner main-nav">
    <ul>
      <% for page in @getCollection("navLinks").toJSON(): %>
        <li class="<%= if page.id is @document.id then 'active' else 'inactive' %>">
          <a href="<%= page.url %>">
            <%= page.title %>
          </a>
        </li>
      <% end %>
    </ul>
  </nav>
</header>
