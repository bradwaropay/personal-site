<header class="header-main">
  <nav class="nav-main">
    <ul>
      <% for page in @getCollection("navLinks").toJSON(): %>
        <li class="<%= if page.id is @document.id then 'active' else 'inactive' %>">
          <a href="<%= page.url %>">
            <%= page.title %>
          </a>
        </li>
      <% end %>
    </ul>
  </nav>
  header
</header>
