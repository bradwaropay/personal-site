<% data = @getYaml().load(@include 'data/_' + @listType + '.yml') %>

<ul class="inline-list <%= @listType %>">

  <% for item in data: %>

    <li class="inline-list__item <%= @listType %>__item"><a class="<%= @listType %>__link <%= item.class %>" href="<%= item.url %>"><%= item.service %></a></li>

  <% end %>

</ul>
