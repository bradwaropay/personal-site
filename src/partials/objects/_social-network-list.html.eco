<% data = @getYaml().load(@include 'data/_social-network-list.yml') %>

<ul class="inline-list social-nav__list social-network-list">

  <% for item in data: %>

    <li class="inline-list__item social-nav__item social-network-list__item"><a class="social-nav__link social-network-list__link" href="<%= item.url %>"><%= item.service %></a></li>

  <% end %>

</ul>
