<section class="post-list">

  <% for post in @getCollection(@postType).toJSON()[@postMin or 0..@postMax]: %>

    <%- @partial("objects/_post-item", {postRobotDate: post.robotDate}, {postHumanDate: post.humanDate}, {postUrl: post.url}, {postTitle: post.title}, {postDescription: post.description}) %>

  <% end %>

</section>
